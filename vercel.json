{
  "version": 2,
  "builds": [
    {
      "src": "portfolio-app/server/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "portfolio-app/vite.config.js",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "portfolio-app/server/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "portfolio-app/dist/$1"
    }
  ]
}
